{
  "name": "Ducktivity",
  "version": "1.0.0",
  "description": "",
  "main": "client/index.js",
  "scripts": {
    "start": "node server/index",
    "heroku-postbuild": "npm run build",
    "test": "mocha --compilers js:babel-register test/**/*.js*",
    "mkdir": "mkdir -p build/css && mkdir -p build/fonts",
    "clean": "rm -rf build",
    "build": "npm run clean && npm run mkdir && npm run build:html && npm run build:js",
    "serve": "nodemon server/index",
    "watch": "npm run watch:html & npm run watch:js",
    "watch:html": "npm run build:html && chokidar index.html -c 'npm run build:html'",
    "build:html": "npm run clean:html && cp index.html build/ && cp fonts/ionicons.eot build/fonts/ && cp fonts/ionicons.svg build/fonts/ && cp fonts/ionicons.ttf build/fonts/  && cp fonts/ionicons.woff build/fonts/ && cp css/ionicons.css build/css/",
    "clean:html": "rm -f build/index.html",
    "watch:js": "npm run build:js && webpack --watch",
    "build:js": "npm run clean:js && webpack",
    "clean:js": "rm -f build/$npm_package_name.$npm_package_version.js build/$npm_package_name.$npm_package_version.js.map",
    "dev": "npm run watch & npm run serve"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "css": "^2.2.1",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "js-cookie": "^2.1.3",
    "less": "^2.7.1",
    "lesshat": "^4.1.0",
    "lodash": "^4.16.6",
    "mongodb": "^2.2.5",
    "mongoose": "^4.5.2",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "passport-http-bearer": "^1.0.1",
    "rc-collapse": "^1.6.8",
    "rc-progress": "^2.0.1",
    "react": "^15.3.2",
    "react-addons-update": "^15.3.2",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.3.2",
    "react-drag-and-drop": "^2.1.0",
    "react-modal": "^1.5.2",
    "react-popup": "^0.4.2",
    "react-progressbar": "^1.3.4",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "chokidar-cli": "^1.2.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "nodemon": "^1.10.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  }
}
